<?xml version="1.0" encoding="utf-8"?>

<androidx.cardview.widget.CardView
    android:id="@+id/card_voice_recycler"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintLeft_toLeftOf="parent"
    app:cardCornerRadius="10dp"
    app:cardElevation="30dp"
    android:layout_margin="@dimen/_5sdp"
    android:layout_width="@dimen/_90sdp"
    android:layout_height="@dimen/_100sdp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:id="@+id/parent11"
        android:background="@drawable/back_ground_of_cards_start_activity1"
        android:layout_height="match_parent">

        <ImageView


            android:padding="@dimen/_8sdp"
            android:id="@+id/image_recycler_voice"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHeight_percent="0.75"
            android:layout_width="match_parent"
            android:layout_height="0dp">

        </ImageView>


        <TextView

            android:id="@+id/text_recycler_voice"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:gravity="center"

            android:textColor="@color/black"
            android:textSize="@dimen/_14sdp"
            android:fontFamily="@font/demi"
            app:layout_constraintHeight_percent="0.25"
            >


        </TextView>








    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.cardview.widget.CardView>

